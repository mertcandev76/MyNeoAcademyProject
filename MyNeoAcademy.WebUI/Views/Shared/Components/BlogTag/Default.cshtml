@model List<ResultTagDTO>
<aside class="single_sidebar_widget tag_cloud_widget">
    <h4 class="widget_title" style="color: #2d2d2d;">Etiket Bulutları</h4>
    <ul class="list">
        @foreach (var tag in Model)
        {
            <li>
                <a href="@Url.Action("Tag", "Blog", new { tag = tag.Name })">@tag.Name</a>
            </li>
        }
    </ul>
</aside>